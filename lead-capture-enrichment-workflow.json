{
  "name": "Lead Capture & Enrichment - Fixed",
  "active": false,
  "nodes": [
    {
      "parameters": {
        "event": "rowAdded",
        "documentId": {
          "__rl": true,
          "value": "YOUR_SHEET_ID",
          "mode": "list",
          "cachedResultName": "Lead Form Responses"
        },
        "sheetName": {
          "__rl": true,
          "value": "gid=0",
          "mode": "list",
          "cachedResultName": "FormResponses"
        },
        "options": {
          "scope": "thisDocument"
        }
      },
      "id": "trigger-new-form",
      "name": "Trigger - New Form Response",
      "type": "n8n-nodes-base.googleSheetsTrigger",
      "typeVersion": 1,
      "position": [300, 300],
      "credentials": {
        "googleSheetsOAuth2Api": {
          "id": "google-sheets-credentials",
          "name": "Google Sheets OAuth2 API"
        }
      }
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": false
          },
          "conditions": [
            {
              "id": "email-check",
              "leftValue": "={{ $json.email }}",
              "rightValue": "@company.com",
              "operator": {
                "type": "string",
                "operation": "contains"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "if-company-email",
      "name": "IF Company Email?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [600, 300]
    },
    {
      "parameters": {
        "resource": "person",
        "operation": "enrich",
        "email": "={{ $json.email }}",
        "additionalFields": {}
      },
      "id": "enrich-clearbit",
      "name": "Enrich via Clearbit",
      "type": "n8n-nodes-base.clearbit",
      "typeVersion": 1,
      "position": [900, 200],
      "credentials": {
        "clearbitApi": {
          "id": "clearbit-credentials",
          "name": "Clearbit API"
        }
      }
    },
    {
      "parameters": {
        "resource": "sheet",
        "operation": "appendOrUpdate",
        "documentId": {
          "__rl": true,
          "value": "YOUR_LEADS_SHEET_ID",
          "mode": "list",
          "cachedResultName": "Leads Database"
        },
        "sheetName": {
          "__rl": true,
          "value": "gid=0",
          "mode": "list",
          "cachedResultName": "Leads"
        },
        "columns": {
          "mappingMode": "defineBelow",
          "value": {
            "Name": "={{ $('Trigger - New Form Response').item.json.name || $json.person?.name?.fullName || 'Unknown' }}",
            "Email": "={{ $('Trigger - New Form Response').item.json.email }}",
            "Company": "={{ $json.person?.employment?.name || $('Trigger - New Form Response').item.json.company || 'N/A' }}",
            "Title": "={{ $json.person?.employment?.title || 'N/A' }}",
            "Location": "={{ $json.person?.location || 'N/A' }}",
            "Enriched": "={{ $json.person ? 'Yes' : 'No' }}",
            "Date Added": "={{ new Date().toISOString().split('T')[0] }}"
          }
        },
        "options": {}
      },
      "id": "save-enriched-lead",
      "name": "Save Enriched Lead",
      "type": "n8n-nodes-base.googleSheets",
      "typeVersion": 4,
      "position": [1200, 200],
      "credentials": {
        "googleSheetsOAuth2Api": {
          "id": "google-sheets-credentials",
          "name": "Google Sheets OAuth2 API"
        }
      }
    },
    {
      "parameters": {
        "resource": "sheet",
        "operation": "appendOrUpdate",
        "documentId": {
          "__rl": true,
          "value": "YOUR_LEADS_SHEET_ID",
          "mode": "list",
          "cachedResultName": "Leads Database"
        },
        "sheetName": {
          "__rl": true,
          "value": "gid=0",
          "mode": "list",
          "cachedResultName": "Leads"
        },
        "columns": {
          "mappingMode": "defineBelow",
          "value": {
            "Name": "={{ $json.name || 'Unknown' }}",
            "Email": "={{ $json.email }}",
            "Company": "={{ $json.company || 'Personal Email' }}",
            "Title": "N/A",
            "Location": "N/A",
            "Enriched": "No",
            "Date Added": "={{ new Date().toISOString().split('T')[0] }}"
          }
        },
        "options": {}
      },
      "id": "save-basic-lead",
      "name": "Save Basic Lead",
      "type": "n8n-nodes-base.googleSheets",
      "typeVersion": 4,
      "position": [1200, 400],
      "credentials": {
        "googleSheetsOAuth2Api": {
          "id": "google-sheets-credentials",
          "name": "Google Sheets OAuth2 API"
        }
      }
    },
    {
      "parameters": {
        "resource": "message",
        "operation": "post",
        "channel": "#leads",
        "text": "üéØ New Lead Captured!\n\n**Name:** {{ $('Trigger - New Form Response').item.json.name || 'Unknown' }}\n**Email:** {{ $('Trigger - New Form Response').item.json.email }}\n**Company:** {{ $json.person?.employment?.name || $('Trigger - New Form Response').item.json.company || 'Personal' }}\n**Title:** {{ $json.person?.employment?.title || 'N/A' }}\n**Enriched:** {{ $json.person ? '‚úÖ Yes' : '‚ùå No' }}",
        "otherOptions": {}
      },
      "id": "notify-slack",
      "name": "Notify Team via Slack",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 2,
      "position": [1500, 300],
      "credentials": {
        "slackApi": {
          "id": "slack-credentials",
          "name": "Slack API"
        }
      }
    }
  ],
  "connections": {
    "Trigger - New Form Response": {
      "main": [
        [
          {
            "node": "IF Company Email?",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "IF Company Email?": {
      "main": [
        [
          {
            "node": "Enrich via Clearbit",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Save Basic Lead",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Enrich via Clearbit": {
      "main": [
        [
          {
            "node": "Save Enriched Lead",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Save Enriched Lead": {
      "main": [
        [
          {
            "node": "Notify Team via Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Save Basic Lead": {
      "main": [
        [
          {
            "node": "Notify Team via Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "pinData": {},
